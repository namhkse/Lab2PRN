@model ODataBookStoreWebClient.Models.Book

@{
	ViewData["Title"] = "Detail";
}

<h1>Book Detail</h1>

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Title</th>
			<th>ISBN</th>
			<th></th>
		</tr>
	</thead>

	<tbody>
			<tr>
				<td>@Model.Id</td>
				<td>@Model.Title</td>
				<td>@Model.ISBN</td>
				<td></td>
			</tr>
		
	</tbody>
</table>